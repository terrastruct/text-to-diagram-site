{
  "name": "D2 Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#0a0f25",
    "editorLineNumber.foreground": "#858585"
  },
  "tokenColors": [
    {
      "settings": {
        "foreground": "#EEF1F6",
        "background": "#0a0f25"
      }
    },
    {
      "name": "errors",
      "scope": "invalid.illegal",
      "settings": {
        "foreground": "#F03A5B",
        "fontStyle": "bold"
      }
    },
    {
      "name": "punctuation",
      "scope": "punctuation",
      "settings": {
        "foreground": "#d0b3ff"
      }
    },
    {
      "name": "string",
      "scope": "string",
      "settings": {
        "foreground": "#30C174"
      }
    },
    {
      "name": "escape",
      "scope": "constant.character.escape",
      "settings": {
        "foreground": "#398741"
      }
    },
    {
      "name": "keys",
      "scope": "entity.name.tag",
      "settings": {
        "foreground": "#7CA1FF"
      }
    },
    {
      "name": "keywords",
      "scope": "keyword",
      "settings": {
        "foreground": "#9B6CFF"
      }
    },
    {
      "name": "number, boolean, null",
      "scope": "constant",
      "settings": {
        "foreground": "#dc5cff"
      }
    },
    {
      "name": "comment",
      "scope": "comment",
      "settings": {
        "foreground": "#7D8295"
      }
    }
  ]
}
